                <groupId>org.jdbi</groupId>
                <artifactId>jdbi-kotlin-sqlobject</artifactId>
                <version>${dep.jdbi.version}</version>
            </dependency>
            <dependency>
                <groupId>org.jdbi</groupId>
                <artifactId>jdbi-moshi</artifactId>
