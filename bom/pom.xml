            </dependency>
            <dependency>
                <groupId>org.jdbi</groupId>
                <artifactId>jdbi-jackson2</artifactId>
                <version>${dep.jdbi.version}</version>
            </dependency>
            <dependency>
                <groupId>org.jdbi</groupId>
                <artifactId>jdbi-jackson3</artifactId>
                <version>${dep.jdbi.version}</version>
            </dependency>
            <dependency>
                <groupId>org.jdbi</groupId>
                <artifactId>jdbi-jodatime2</artifactId>
                <version>${dep.jdbi.version}</version>
            </dependency>
